---
import Container from '@components/Container.astro';
import BaseLayout from './BaseLayout.astro'
import ContentWithNav from '@components/ContentWithNav.astro';

const tableOfContent = [
    { title: 'Shulk' },
    {
        title: 'Introduction',
        slug: '/docs/'
    },
    {
        title: 'Get started',
        slug: '/docs/get-started/'
    },
    { title: 'Basics' },
    {
        title: 'Tagged unions',
        slug: '/docs/tagged-unions/'
    },
    {
        title: 'Pattern matching',
        slug: '/docs/pattern-matching/'
    },
    { title: 'Monads' },
    {
        title: 'Result',
        slug: '/docs/result/'
    },
    {
        title: 'Maybe',
        slug: '/docs/maybe/'
    },
    {
        title: 'Loading',
        slug: '/docs/loading/'
    }, 
    { title: 'Async' },
    {
        title: 'Procedure',
        slug: '/docs/procedure/'
    },
    {
        title: 'Concurrently',
        slug: '/docs/concurrently/'
    },
    { title: 'Others' },
    {
        title: 'Wrappers',
        slug: '/docs/wrappers/',
    },
   
];

   
const pathname = Astro.url.pathname
 
const { frontmatter } = Astro.props;
---

<BaseLayout title={frontmatter.title + '- Shulk'}>
    <Container>
        <ContentWithNav     
            tableOfContent={tableOfContent}
            selected={pathname}
            title={frontmatter.title}
        >
            <slot />
        </ContentWithNav>
    </Container>
</BaseLayout>
	



